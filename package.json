{
  "name": "kairosdb",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env development",
    "build:prod": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "build:dev": "webpack -c ./.config/webpack/webpack.config.ts --env development",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix && prettier --write --list-different .",
    "e2e": "playwright test",
    "server": "docker compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "author": "Arp Networking",
  "license": "Apache-2.0",
