{
    "compilerOptions": {
        "module": "amd",
        "target": "es6",
        "noImplicitAny": false,
        "removeComments": false,
        "preserveConstEnums": true,
        "outDir": ".",
        "rootDir": ".",
        "sourceMap": false
    },
    "include": [
        "*",
        "**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}
